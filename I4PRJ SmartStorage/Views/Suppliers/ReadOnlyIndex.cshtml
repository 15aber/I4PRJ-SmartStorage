@model IEnumerable<I4PRJ_SmartStorage.Models.Domain.Supplier>

@{
    ViewBag.Title = "Suppliers";
}


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>List of Suppliers</h5>
                </div>
                <div class="ibox-content">

<table class="table table-striped table-bordered table-hover" id="datatable-table">
  <thead>  <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Updated)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ByUser)
        </th>
    
    </tr></thead>

@foreach (var item in Model) {
 <tbody>   <tr>
  
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Updated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ByUser)
        </td>

    </tr></tbody>
}

</table>

                </div>
            </div>
        </div>
    </div>
 </div>